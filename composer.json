{
    "name": "rollerworks/search-bundle",
    "description": "Rollerworks Search Bundle",
    "keywords": ["filter", "search", "rollerworks", "symfony", "bundle"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "rollerworks/search": "~1.0@dev",
        "symfony/framework-bundle": "~2.1",
        "symfony/finder": "~2.1"
    },
    "require-dev": {
        "doctrine/orm": "~2.4",
        "doctrine/doctrine-bundle": ">=1.1",
        "rollerworks/cache-bundle": "1.0.*@dev",
        "symfony/class-loader": "~2.1",
        "jms/metadata": ">=1.1.1",
        "phpunit/phpunit": "~3.7",
        "matthiasnoback/symfony-config-test": "0.*",
        "matthiasnoback/symfony-dependency-injection-test": "0.*"
    },
    "suggest": {
        "rollerworks/cache-bundle": "Session based caching for increased performance"
    },
    "autoload": {
        "psr-0": { "Rollerworks\\Bundle\\SearchBundle": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
