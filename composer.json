{
    "name": "rollerworks/search-symfony-validator",
    "description": "Symfony validator extension for RollerworksSearch",
    "keywords": ["rollerworks", "search", "validation", "symfony"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/Rollerworks/search-symfony-validator/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "rollerworks/search": "^2.0@dev,>=2.0.0-ALPHA4",
        "symfony/validator": "^3.3.6 || ^4.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.3.6 || ^4.0",
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\Search\\Extension\\Symfony\\Validator\\": "src/"
        },
        "exclude-from-classmap": ["test/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Rollerworks\\Component\\Search\\Extension\\Symfony\\Validator\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
